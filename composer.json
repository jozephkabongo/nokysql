{
    "name": "jozephkabongo/nokysql",
    "description": "NokySQL is a lightweight, high-performance SQL Query Builder for PHP that bridges the gap between raw SQL and full ORMs.",
    "version": "1.0.0",
    "type": "library",
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/jozephkabongo/nokysql"
        }
    ],
    "readme": "README.md",
    "keywords": [
        "mysql", "sqlite", "postdresql", "sql", "data base", "query",
        "queries", "php", "php queries", "php mysql", "php data base", "database"
    ],
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NokySQL\\": "src/NokySQL/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jozeph Kabongo",
            "email": "kabongojozeph@gmail.com",
            "role": "developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "10.0"
    },
    "time": "2025-03-25"
}
